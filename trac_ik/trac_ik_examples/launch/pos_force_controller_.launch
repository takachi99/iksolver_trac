    </group>
    <!-- <node pkg="trac_ik_examples" name="force_convert" type="force_convert"/> -->
    <!-- <node pkg="trac_ik_examples" name="reset_ftsensor" type="reset_ftsensor.py" output="screen"/> -->
    <node pkg="trac_ik_examples" name="track_ik_jointpub" type="trac_ik_jointpub" output="screen"/>
    <!-- <node pkg="trac_ik_examples" name="pf_hyb_controller" type="pf_hyb_controller" output="screen"/> -->
    <node name="Lowpass_wrench_torque_plot"
            pkg="rqt_plot"
